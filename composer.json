{
    "name": "alahaxe/honeypot-bundle",
    "description": "Honeypot bundle for SF",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alahaxe\\HoneypotBundle\\": "src/",
            "Alahaxe\\HoneypotBundle\\Tests\\": "tests/"
        }
    },
